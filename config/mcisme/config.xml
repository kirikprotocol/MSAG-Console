      <section name="NoReply">
        <param name="cause" type="int">19</param>
        <param name="inform" type="bool">true</param>
      </section>

      <section name="Other">
        <param name="cause" type="int">21</param>
        <param name="inform" type="bool">false</param>
      </section>

      <section name="Unconditional">
        <param name="cause" type="int">21</param>
        <param name="inform" type="bool">false</param>
      </section>
