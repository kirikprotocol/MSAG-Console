<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE counters SYSTEM "counters.dtd">

<counters>
    <param name="last used template id" type="int">1</param>
    <param name="last used ca_table id" type="int">1</param>

    <templates>
	
	<template id="0" type="ACCUMULATOR">
	    <name>SimpleAcc</name>
	    <ca id="0"/>
	</template>
	
	<template id="1" type="AVERAGE">
	    <name>SimpleAvg</name>
	    <param name="slot" type="int">10</param>
	    <ca id="1"/>
	</template>
	
    </templates>

    <ca_tables>
    
	<ca_table id="0" system="true">
	    <name>DefaultCATable</name>
	    <limits min="0" max="1000">
		<limit procent="10" severity="NORMAL"/>
		<limit procent="75" severity="WARNING"/>
		<limit procent="95" severity="CRITICAL"/>
	    </limits>
	    <param name="some_param" type="string">101</param>
	</ca_table>

	<ca_table id="1">
	    <name>UserCATable</name>
	    <limits min="100" max="500">
		<limit procent="5" severity="NORMAL"/>
		<limit procent="50" severity="WARNING"/>
		<limit procent="75" severity="MAJOR"/>
	    </limits>
	</ca_table>
	
    </ca_tables>
    
</counters>