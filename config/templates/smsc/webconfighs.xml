<?xml version="1.0" encoding="WINDOWS-1251"?>
<!DOCTYPE config SYSTEM "file://configuration.dtd">

<config>
  <section name="installation">
    <param name="type" type="string">HS</param>
    <param name="mirrorpath" type="string">c:/temp/mirror/</param>
  </section>

  <section name="nodes">
    <section name="sunfire">
      <param name="id" type="string">1</param>
    </section>

    <section name="phoenix">
      <param name="id" type="string">2</param>
    </section>
  </section>

  <section name="categories">
    <param name="1" type="string">category1</param>
    <param name="2" type="string">supercategory</param>
    <param name="3" type="string">super puper category mega giga</param>
    <param name="last used category id" type="int">3</param>
  </section>
  
  <section name="console">
    <param name="enabled" type="bool">false</param>
    <param name="humanPort" type="int">15925</param>
    <param name="scriptPort" type="int">15926</param>
  </section>

  <section name="daemon">
      <param name="host" type="string">sunfire</param>
      <param name="port" type="int">9022</param>
      <param name="services folder" type="string">z:/install/services</param>
  </section>

  <section name="perfmon">
    <param name="appletport" type="int">15921</param>
    <param name="smscport" type="int">15920</param>
  </section>

  <section name="topmon">
    <param name="appletport" type="int">15923</param>
    <param name="smscport" type="int">15922</param>
  </section>

  <section name="profiler">
    <section name="jdbc">
      <param name="driver" type="string">oracle.jdbc.driver.OracleDriver</param>
      <param name="source" type="string">jdbc:oracle:thin:@max:1521:ORCL</param>
      <param name="user" type="string">smsc_starkom</param>
      <param name="password" type="string">smsc_starkom</param>
    </section>
  </section>
  
  <section name="providers">
    <param name="1" type="string">hello</param>
    <param name="2" type="string">mark</param>
    <param name="3" type="string">burger</param>
    <param name="last used provider id" type="int">3</param>
  </section>

  <section name="smsc">
    <param name="config folder" type="string">z:/install/services/SMSC/conf</param>
    <param name="host" type="string">sunfire</param>
    <param name="port" type="int">15911</param>
  </section>

  <section name="system">
    <param name="localization file" type="string">c:/smsc_web/conf/localization.xml</param>
    <param name="users file" type="string">c:/smsc_web/conf/users.xml</param>
    <param name="webapp folder" type="string">c:/smsc_web/webapps/smsc</param>
    <param name="work folder" type="string">c:/smsc_web/temp</param>
  </section>
 
  <section name="statsave_datasource">
      <param name="source" type="string">jdbc:oracle:thin:@max:1521:ORCL</param>
      <param name="driver" type="string">oracle.jdbc.driver.OracleDriver</param>
      <param name="user" type="string">smsc_starkom</param>
      <param name="pass" type="string">smsc_starkom</param>
      <param name="tables_prefix" type="string">sms_stat</param>
  </section>  

  <section name="opersave_datasource">
    <param name="driver" type="string">oracle.jdbc.driver.OracleDriver</param>
    <param name="pass" type="string">smsc_starkom</param>
    <param name="source" type="string">jdbc:oracle:thin:@max:1521:ORCL</param>
    <param name="tables_prefix" type="string">oper_sms</param>
    <param name="user" type="string">smsc_starkom</param>
  </section>

  <section name="archsave_datasource">
    <param name="driver" type="string">oracle.jdbc.driver.OracleDriver</param>
    <param name="pass" type="string">smsc_starkom</param>
    <param name="source" type="string">jdbc:oracle:thin:@max:1521:ORCL</param>
    <param name="tables_prefix" type="string">arch_sms</param>
    <param name="user" type="string">smsc_starkom</param>
  </section>

  <section name="system">
    <param name="localization file" type="string">c:/smsc_web/conf/localization.xml</param>
    <param name="users file" type="string">c:/smsc_web/conf/users.xml</param>
    <param name="webapp folder" type="string">c:/smsc_web/webapps/smsc</param>
    <param name="work folder" type="string">c:/smsc_web/temp</param>
  </section>

  <section name="snmp">
    <param name="csvFileDir" type="string">@localstore.dir@/snmp</param>
    <param name="csvFileRollInterval" type="int">60</param>
  </section>
  
  <section name="personalization">
    <param name="host" type="string">@host</param>
    <param name="port" type="string">@port</param>
    <param name="timeout" type="string">@timeout</param>
    <param name="minConnections" type="string">@minConnections</param>
    <param name="maxConnections" type="string">@maxConnections</param>
    <param name="checkConnections" type="string">@checkConnections</param>
    <param name="backgroundConnectMode" type="string">@backgroundConnectMode</param>
    <param name="maxIdleTime" type="string">@maxIdleTime</param>
    <param name="maxClientConnectTime" type="string">@maxClientConnectMode</param>
    <param name="poolControllerInterval" type="string">@poolControllerInterval</param>
    <param name="maxShutdownTime" type="string">@maxShutdownTime</param>
  </section>
</config>
