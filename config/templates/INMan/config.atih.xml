<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE config SYSTEM "file://configuration.dtd">

<!-- INMan interacts the HLR -->
<config>
  <section name="HLR_interaction"> <!-- IN platform interaction parameters -->
    <param name="scf_ssn" type="int">146</param>
    <param name="scf_address" type="string">79139869981</param>  <!-- InMan address -->
    <param name="hlr_ssn" type="int">6</param>
    <!-- OPTIONAL PARAMETERS follow:: -->
    <param name="ss7UserId" type="int">03</param>     <!-- PortSS7 user id [1..20], default: 03  -->
    <param name="mapTimeout" type="int">20</param>   <!-- timeout on MAP operations, units: seconds, max: 65535, default 20 -->
 </section>
</config>

<!--
SCCP configuration

GT           =  SSN

79139869981  = 146
791398699810 =   7
791398699811 = 160
79139869998  =   6
79139869999  =   8
-->

