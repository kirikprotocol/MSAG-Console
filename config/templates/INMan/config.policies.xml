<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE config SYSTEM "file://configuration.dtd">
<!--  "$Id$" -->
<!-- configuration for INMan v0.8.0 or higher -->
<config>

  <section name="AbonentPolicies">
  <!--  policy value is a CSV(:) list of  two parts(first one is optional):

       1) prioritized CSV(,) List of abonent providers to use (defined in
          section AbonentProviders) for getting abonent contract type
          (possibly with gsmSCF parms),

       2) CSV(,) list of IN-platform (defined in section IN-platforms)
         NOTE: * is a special symbol, replacing all defined IN-platforms
  -->
    <section name="MTC_Default">
      <param name="policy" type="string">HLR_CH_SRI : *</param> <!-- get contract from gsmSCF, No provider at all !!!  -->
      <section name="AddressPools">
        <param name="pool_RU" type="string">79?????????</param>  <!-- address pool mask -->
      </section>
      <!-- param name="active" type="bool">false</param -->    <!-- use or ignore this policy, default: true -->
    </section>

    <section name="MTC_Nsk_SRI"> <!--
    get contract/gsmSCF from HLR using SRI, serviceKey from INMan config
    -->
      <param name="policy" type="string">HLR_CH_SRI : *</param>
      <section name="AddressPools">
        <param name="pool_Nsk_913" type="string">7913???????</param>  <!-- address pool mask -->
      </section>
      <!-- param name="active" type="bool">false</param -->    <!-- use or ignore this policy, default: true -->
    </section>

    <section name="MTC_Msk_ATSI"> <!-- get contract/gsmSCF from HLR using ATSI -->
      <param name="policy" type="string">HLR_ATSI, HLR_CH_SRI : *</param>
      <section name="AddressPools">
        <param name="pool_Msk_tst_0" type="string">79104075344</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_1" type="string">79169300003</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_2" type="string">79165603331</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_3" type="string">79166699601</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_4" type="string">79857603330</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_5" type="string">79166803330</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_6" type="string">79857860099</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_7" type="string">79161690056</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_8" type="string">79166199999</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_9" type="string">79162624625</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_10" type="string">79167595907</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_11" type="string">79160305277</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_12" type="string">79853646295</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_13" type="string">79163841468</param>  <!-- address pool mask -->
        <param name="pool_Msk_tst_14" type="string">79169037692</param>  <!-- address pool mask -->
      </section>
      <!-- param name="active" type="bool">false</param -->    <!-- use or ignore this policy, default: true -->
    </section>

  </section> <!-- AbonentPolicies -->

  <section name="AbonentProviders"> <!-- abonent contract/gsmSCF params  providers -->
      <section name="HLR_CH_SRI">
        <param name="loadup" type="string">uid: icsIAPrvdSRI</param>
        <section name="Config">
          <param name="tcapUser" type="string">TCAPUserSRI</param> <!--
          section name for TCAP user configuration this provider uses
          -->
          <param name="iniThreads" type="int">5</param>  <!--
          initial number of threads serving IAPQUeries
          -->
        </section>
      </section>

       <section name="HLR_ATSI">
        <param name="loadup" type="string">uid: icsIAPrvdATSI</param>
        <section name="Config">
          <param name="tcapUser" type="string">TCAPUserATSI</param> <!--
          section name for TCAP user configuration this provider uses
          -->
          <param name="iniThreads" type="int">5</param>  <!--
          initial number of threads serving IAPQUeries
          -->
        </section>
      </section>

  </section> <!-- AbonentProviders -->

</config>

