<project name="External sme build" default="help" basedir=".">
  <target name="help">
    <echo>
Modules:
      dbSme_distr
      dbSme_update
    </echo>
  </target>
  <target name="dbSme_distr">
    <ant dir="dbSme" antfile="build.xml" target="distr"/>
  </target>
  <target name="dbSme_update">
    <ant dir="dbSme" antfile="build.xml" target="update"/>
  </target>
</project>
