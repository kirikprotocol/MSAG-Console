<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
    "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.2//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_1.dtd" >
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

  <application>
    <default-render-kit-id>
      org.apache.myfaces.trinidad.core
    </default-render-kit-id>

    <locale-config>
      <default-locale>ru</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.web.resources.Informer</base-name>
      <var>msg</var>
    </resource-bundle>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.admin.informer.InformerSettings</base-name>
      <var>informerSettings</var>
    </resource-bundle>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.admin.smsc.Smsc</base-name>
      <var>smscBundle</var>
    </resource-bundle>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.admin.regions.Region</base-name>
      <var>regionBundle</var>
    </resource-bundle>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.admin.users.User</base-name>
      <var>userBundle</var>
    </resource-bundle>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.admin.delivery.Delivery</base-name>
      <var>deliveryBundle</var>
    </resource-bundle>

  </application>

  <converter>
    <converter-id>timeZoneConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.TimeZoneNameConverter</converter-class>
  </converter>

  <converter>
    <converter-id>timeZoneIdConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.TimeZoneIdConverter</converter-class>
  </converter>

  <converter>
    <converter-id>daemonsConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.DaemonsConverter</converter-class>
  </converter>


  <converter>
    <converter-id>deliveryStatusConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.delivery.DeliveryStatusConverter</converter-class>
  </converter>


  <converter>
    <converter-id>deliveryTypeConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.delivery.DeliveryTypeConverter</converter-class>
  </converter>


  <converter>
    <converter-id>weekDaysConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.WeekDaysConverter</converter-class>
  </converter>


  <converter>
    <converter-id>addressConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.AddressConverter</converter-class>
  </converter>

  <converter>
    <converter-id>messageStateConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.delivery.MessageStateConverter</converter-class>
  </converter>

  <converter>
    <converter-id>errorCodeConverter</converter-id>
    <converter-class>mobi.eyeline.informer.web.controllers.ErrorCodeConverter</converter-class>
  </converter>

  <render-kit>
    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>collapsingGroup</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.collapsing_group.CollapsingGroupRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dynamicTable</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.dynamic_table.DynamicTableRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>menuBar</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.menubar.MenuBarRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>buttonsPanel</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.buttons_panel.ButtonsPanelRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTable</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.data_table.DataTableRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTableColumn</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.data_table.ColumnRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTableRow</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.data_table.RowRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>updatableContent</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.updatable_content.UpdatableContentRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>pageCalendar</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.page_calendar.PageCalendarRenderer</renderer-class>
    </renderer>
  </render-kit>

