<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
    "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.2//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_1.dtd" >
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

  <application>
    <default-render-kit-id>
      org.apache.myfaces.trinidad.core
    </default-render-kit-id>

    <locale-config>
      <default-locale>ru</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>

    <resource-bundle>
      <base-name>mobi.eyeline.informer.web.resources.Informer</base-name>
      <var>msg</var>
    </resource-bundle>

  </application>

  <!--<converter>-->
  <!--<converter-id>smpp_status_converter</converter-id>-->
  <!--<converter-class>mobi.eyeline.informer.web.controllers.reschedule.SmppStatusConverter</converter-class>-->
  <!--</converter>-->


  <render-kit>
    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>collapsingGroup</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.collapsing_group.CollapsingGroupRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dynamicTable</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.dynamic_table.DynamicTableRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>buttonsPanel</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.buttons_panel.ButtonsPanelRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTable</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.data_table.DataTableRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTableColumn</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.data_table.ColumnRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTableRow</renderer-type>
      <renderer-class>mobi.eyeline.informer.web.components.data_table.RowRenderer</renderer-class>
    </renderer>
  </render-kit>

  <managed-bean>
    <managed-bean-name>topMenu</managed-bean-name>
    <managed-bean-class>mobi.eyeline.informer.web.controllers.TopMenu</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>

  <managed-bean>
    <managed-bean-name>logout</managed-bean-name>
    <managed-bean-class>mobi.eyeline.informer.web.controllers.LogoutController</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>


  <managed-bean>
    <managed-bean-name>journal</managed-bean-name>
    <managed-bean-class>mobi.eyeline.informer.web.controllers.journal.JournalController</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>



  <navigation-rule>
    <navigation-case>
      <from-outcome>INDEX</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-outcome>JOURNAL</from-outcome>
      <to-view-id>/journal/index.xhtml</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>


</faces-config>