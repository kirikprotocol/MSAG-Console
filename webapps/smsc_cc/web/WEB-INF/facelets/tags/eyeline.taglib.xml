<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE facelet-taglib PUBLIC
    "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
    "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
  <!--<library-class>ru.novosoft.smsc.web.components.EyelineTagLib</library-class>-->

  <namespace>http://taglib.eyeline.mobi</namespace>

  <tag>
    <tag-name>collapsingGroup</tag-name>
    <component>
      <component-type>collapsingGroup</component-type>
      <renderer-type>collapsingGroup</renderer-type>
      <handler-class>ru.novosoft.smsc.web.components.collapsing_group.CollapsingGroupHandler</handler-class>
    </component>
  </tag>

  <tag>
    <tag-name>dynamicTable</tag-name>
    <component>
      <component-type>dynamicTable</component-type>
      <renderer-type>dynamicTable</renderer-type>
      <handler-class>ru.novosoft.smsc.web.components.dynamic_table.DynamicTableHandler</handler-class>
    </component>
  </tag>

  <tag>
    <tag-name>dtTextColumn</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.dynamic_table.TextColumnTagHandler</handler-class>
  </tag>

  <tag>
    <tag-name>dtSelectColumn</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.dynamic_table.SelectColumnTagHandler</handler-class>
  </tag>

  <tag>
    <tag-name>pageTitle</tag-name>
    <source>page_title.xhtml</source>
  </tag>

  <tag>
    <tag-name>buttons</tag-name>
    <component>
      <component-type>buttons</component-type>
      <renderer-type>buttons</renderer-type>
      <handler-class>ru.novosoft.smsc.web.components.buttons.ButtonsHandler</handler-class>
    </component>
  </tag>

  <tag>
    <tag-name>button</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.buttons.ButtonHandler</handler-class>
  </tag>

  <tag>
    <tag-name>space</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.buttons.SpaceHandler</handler-class>
  </tag>

  <tag>
    <tag-name>pagedTable</tag-name>
    <component>
      <component-type>pagedTable</component-type>
      <renderer-type>pagedTable</renderer-type>
      <handler-class>ru.novosoft.smsc.web.components.paged_table.PagedTableHandler</handler-class>
    </component>
  </tag>

  <tag>
    <tag-name>ptTextColumn</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.paged_table.TextColumnTagHandler</handler-class>
  </tag>

  <tag>
    <tag-name>ptImageColumn</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.paged_table.ImageColumnTagHandler</handler-class>
  </tag>

  <tag>
    <tag-name>ptFilter</tag-name>
    <handler-class>ru.novosoft.smsc.web.components.paged_table.FilterTagHandler</handler-class>
  </tag>

</facelet-taglib>