srcdef[RouteGenCommand cmd] { // Special command required !!!
    RouteSrcDef def = new RouteSrcDef();
}
	: ((OPT_SUBJ {
	    def.setType(RouteSrcDef.TYPE_SUBJECT);
	    def.setSrc(getnameid("Subject name"));
