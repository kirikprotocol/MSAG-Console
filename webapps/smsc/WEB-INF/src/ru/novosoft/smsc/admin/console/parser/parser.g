	| TGT_ALIAS	cmd = delalias
	| TGT_SUBJECT	cmd = delsubject
	| TGT_PROFILE	cmd = delprofile
	| TGT_PRINCIPAL	cmd = delprincipal
	| TGT_ACL	cmd = delacl
	| TGT_DL	cmd = deldl
	| TGT_DLSUB	cmd = deldlsubmitter
	| TGT_DLMEM	cmd = deldlmember
	;
