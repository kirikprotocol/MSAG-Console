<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
    "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.2//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_1.dtd" >
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

  <managed-bean>
    <managed-bean-name>componentsResource</managed-bean-name>
    <managed-bean-class>mobi.eyeline.util.jsf.components.ResourceBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>

  <lifecycle>
    <phase-listener>mobi.eyeline.util.jsf.components.ResourcePhaseListener</phase-listener>
  </lifecycle>

  <application>
    <resource-bundle>
      <base-name>mobi.eyeline.util.jsf.components.resources.Messages</base-name>
      <var>eyelineComponentsMsg</var>
    </resource-bundle>
  </application>

  <converter>
    <converter-id>eyelineDateConverter</converter-id>
    <converter-class>mobi.eyeline.util.jsf.components.DateConverter</converter-class>
  </converter>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.input_time</component-type>
    <component-class>mobi.eyeline.util.jsf.components.input_time.InputTime</component-class>
  </component>

  <validator>
    <validator-id>mobi.eyeline.util.jsf.components.validateTimeInterval</validator-id>
    <validator-class>mobi.eyeline.util.jsf.components.input_time.TimeIntervalValidator</validator-class>
  </validator>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.message</component-type>
    <component-class>mobi.eyeline.util.jsf.components.message.Message</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.collapsing_group</component-type>
    <component-class>mobi.eyeline.util.jsf.components.collapsing_group.CollapsingGroup</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.buttons_panel</component-type>
    <component-class>mobi.eyeline.util.jsf.components.buttons_panel.ButtonsPannel</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.buttons_panel.button</component-type>
    <component-class>mobi.eyeline.util.jsf.components.buttons_panel.Button</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.buttons_panel.space</component-type>
    <component-class>mobi.eyeline.util.jsf.components.buttons_panel.Space</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.updatable_content</component-type>
    <component-class>mobi.eyeline.util.jsf.components.updatable_content.UpdatableContent</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.progress_bar</component-type>
    <component-class>mobi.eyeline.util.jsf.components.progress_bar.ProgressBar</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.input_date</component-type>
    <component-class>mobi.eyeline.util.jsf.components.input_date.InputDate</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.menubar</component-type>
    <component-class>mobi.eyeline.util.jsf.components.menubar.MenuBar</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.menubar.item</component-type>
    <component-class>mobi.eyeline.util.jsf.components.menubar.MenuBarItem</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.menubar.delimiter</component-type>
    <component-class>mobi.eyeline.util.jsf.components.menubar.MenuBarDelimiter</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.dynamic_table</component-type>
    <component-class>mobi.eyeline.util.jsf.components.dynamic_table.DynamicTable</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.data_table</component-type>
    <component-class>mobi.eyeline.util.jsf.components.data_table.DataTable</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.data_table.column</component-type>
    <component-class>mobi.eyeline.util.jsf.components.data_table.Column</component-class>
  </component>

  <component>
    <component-type>mobi.eyeline.util.jsf.components.data_table.row</component-type>
    <component-class>mobi.eyeline.util.jsf.components.data_table.Row</component-class>
  </component>


  <render-kit>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>pageCalendar</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.page_calendar.PageCalendarRenderer</renderer-class>
    </renderer>


    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>lines</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.graph.LinesRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>lineChart</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.graph.LinesRenderer</renderer-class>
    </renderer>

  </render-kit>


</faces-config>