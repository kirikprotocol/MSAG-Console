<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
    "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.2//EN"
    "http://java.sun.com/dtd/web-facesconfig_1_1.dtd" >
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

  <managed-bean>
    <managed-bean-name>componentsResource</managed-bean-name>
    <managed-bean-class>mobi.eyeline.util.jsf.components.ResourceBean</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
  </managed-bean>

  <lifecycle>
    <phase-listener>mobi.eyeline.util.jsf.components.ResourcePhaseListener</phase-listener>
  </lifecycle>

  <application>
    <resource-bundle>
      <base-name>mobi.eyeline.util.jsf.components.resources.Messages</base-name>
      <var>eyelineComponentsMsg</var>
    </resource-bundle>
  </application>

  <converter>
    <converter-id>eyelineDateConverter</converter-id>
    <converter-class>mobi.eyeline.util.jsf.components.DateConverter</converter-class>
  </converter>

  <render-kit>
    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>collapsingGroup</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.collapsing_group.CollapsingGroupRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dynamicTable</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.dynamic_table.DynamicTableRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>menuBar</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.menubar.MenuBarRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>menu</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.menubar.MenuBarRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>buttonsPanel</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.buttons_panel.ButtonsPanelRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTable</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.data_table.DataTableRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTableColumn</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.data_table.ColumnRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>dataTableRow</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.data_table.RowRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>updatableContent</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.updatable_content.UpdatableContentRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>pageCalendar</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.page_calendar.PageCalendarRenderer</renderer-class>
    </renderer>


    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>lines</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.graph.LinesRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>lineChart</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.graph.LinesRenderer</renderer-class>
    </renderer>

    <renderer>
      <component-family>Eyeline</component-family>
      <renderer-type>inputTime</renderer-type>
      <renderer-class>mobi.eyeline.util.jsf.components.input_time.InputTimeRenderer</renderer-class>
    </renderer>

  </render-kit>


</faces-config>