# INMan and INMan testing console executables

inman/inmanTest inmanTest -linman -linman-interaction-msgbill -linman-interaction-msgdtcr -linman-interaction -linman-common -linman-storage $(SOCKETLIB) -lrt -L$(SMSC_BUILDDIR)/bin -lutil  -lcore-threads -lthread -llogger -lcore-network -lnsl -lutil @liblogger.so

switch EIN_SS7_LINKAGE
static:\
inman/inman inman $(SOCKETLIB) -linman -linman-services -linman-inap -L$(SS7_STACK_HOME)/lib -l$(SS7_LIBRARY_NAME) -linman-services-tcpsrv -linman-services-tmwatch -linman-interaction -linman-common -lutil-config -lutil-xml $(XERCES_LIB) $(__LIBRARIES) -lrt -L$(SMSC_BUILDDIR)/bin -lcore-threads -lthread -llogger -lcore-network -lnsl -ldl -lutil-mirrorfile -lutil -lutil-URCdb @liblogger.so

default:\
inman/inman inman $(SOCKETLIB) -linman -linman-services -linman-services-tcpsrv -linman-services-tmwatch -linman-interaction -linman-common -lutil-config -lutil-xml $(XERCES_LIB) $(__LIBRARIES) -lrt -L$(SMSC_BUILDDIR)/bin -lcore-threads -lthread -llogger -lcore-network -lnsl -ldl -lutil-mirrorfile -lutil -lutil-URCdb @liblogger.so
end switch

