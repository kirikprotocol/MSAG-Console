# INMan executable

switch INMAN_ICS_LINKAGE
static:\
inman/inman inman $(SOCKETLIB) -linman -linman-services -linman-services-tcpsrv -linman-services-tmwatch -linman-services-abdtcr -linman-interaction-msgdtcr -linman-services-iapmgr -linman-services-iapSRI -linman-abprov  -linman-abprov-facility -linman-inap-map_chsri -linman-comp-map_chsri -linman-services-scheduler -linman-incache-solid -linman-services-smbill -linman-interaction-msgbill -linman-storage -linman-services-abcache -linman-incache-solid -linman-services-tcapdsp -linman-inap -L$(SS7_STACK_HOME)/lib -l$(SS7_LIBRARY_NAME) -linman-inap-cap_sms -linman-comp-cap_sms -linman-interaction -linman-comp -linman-codec_inc-cap -linman-codec_inc-map -linman-codec_inc-in_cs -linman-codec_inc-ros -linman-common -linman-asn1rt -lutil-config -lutil-xml $(XERCES_LIB) -lrt -L$(SMSC_BUILDDIR)/bin -lcore-threads -llogger -lcore-network -lnsl -ldl -lutil-mirrorfile -lutil-csv -lutil -lutil-URCdb @liblogger.so
staticSS7:\
inman/inman inman $(SOCKETLIB) -linman -linman-services -linman-services-tcpsrv -linman-services-tcapdsp -linman-inap -L$(SS7_STACK_HOME)/lib -l$(SS7_LIBRARY_NAME) -linman-comp -linman-services-tmwatch -linman-interaction -linman-common -lutil-config -lutil-xml $(XERCES_LIB) -lrt -L$(SMSC_BUILDDIR)/bin -lcore-threads -llogger -lcore-network -lnsl -ldl -lutil-mirrorfile -lutil-csv -lutil -lutil-URCdb @liblogger.so
default:\
inman/inman inman $(SOCKETLIB) -linman -linman-services -linman-services-tcpsrv -linman-services-tmwatch -linman-interaction -linman-common -lutil-config -lutil-xml $(XERCES_LIB) $(__LIBRARIES) -lrt -L$(SMSC_BUILDDIR)/bin -lcore-threads -llogger -lcore-network -lnsl -ldl -lutil-mirrorfile -lutil-csv -lutil -lutil-URCdb @liblogger.so
end switch

