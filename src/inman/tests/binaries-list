# INMan testing console executables

NETSOCK_LIBS=-lcore-network $(SOCKETLIB) -lnsl
TCPSRV_LIBS=-linman-interaction-tcpserver -linman-interaction-asynconn

inman/inmanTest inmanTest -L$(SMSC_BUILDDIR)/bin -linman-tests -linman \
  -linman-interaction-msgbill -linman-interaction-msgdtcr \
  -linman-interaction-serializer %(TCPSRV_LIBS) \
  -linman-storage -linman-abprov -linman-comp -linman-common \
  -llogger @liblogger.so \
  %(NETSOCK_LIBS) -lcore-threads -lcore-buffers \
  -lutil -lpthread -lrt -ldl


