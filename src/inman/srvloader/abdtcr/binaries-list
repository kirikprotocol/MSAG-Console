# Abonent detector service shared library loader
#
# format: binary_name source_name libraries

# using global setting of service linkage
switch INMAN_ICS_LINKAGE
static: #no need for .so loader, service is statically linked into libinman
default: \
inman/ldlib/libinman_ics_abdtcr.so AbntDtcrLoader $(SHARED_OPT) -linman-services-abdtcr -linman-interaction-asyncmgr -linman-interaction-msgdtcr  -linman-interaction-serializer -linman-abprov -lutil -L$(SMSC_BUILDDIR)/bin/inman/ldlib  -linman_ics_iapmgr @inman/ldlib/libinman_ics_iapmgr.so -ldl -lrt
end switch

