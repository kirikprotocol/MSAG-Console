infosme/infosme2   	  InfoSme \
 -linfosme-v2 \
 -ladmin-protocol -ladmin-service \
 -lsms \
 -lcore-network $(SOCKETLIB) -lnsl \
 -lcore-threads \
 -lutil-regexp \
 -lutil-recoder-recode_dll \
 -lutil-templates \
 -lutil-csv \
 -lutil-mirrorfile \
 -lutil-config-route \
 -lutil-config-region \
 -lutil-config \
 -lutil-xml $(XERCES_LIB) \
 @liblogger.so -L$(SMSC_BUILDDIR)/bin -llogger \
 -lutil \
 -llicense-check \
 $(__LIBRARIES) \
 $(SSL_LIB) \
switch INFOSMEDB
NO: \
default: -ldb -L$(SMSC_BUILDDIR)/bin/db/exceptions/ -ldb_exc @db/exceptions/libdb_exc.so \
endswitch
 -lpthread -lrt -ldl

