<!ENTITY % sectionbody "(timeout | msc | sme | pipe )*">

<!ELEMENT config %sectionbody;>
<!ATTLIST config 
    ussd_ssn  CDATA #REQUIRED
    smsc_ssn  CDATA #REQUIRED
    mqueuelen CDATA #REQUIRED
    autostart CDATA #REQUIRED
	    
	    
>

<!ELEMENT timeout (#PCDATA)>
<!ATTLIST timeout val CDATA #REQUIRED>

<!ELEMENT msc (#PCDATA)>
<!ATTLIST msc address CDATA #REQUIRED>

<!ELEMENT sme (#PCDATA)>
<!ATTLIST sme address CDATA #REQUIRED
	      sid CDATA #REQUIRED	
	      host CDATA #REQUIRED	
	      port CDATA #REQUIRED	
	      password CDATA #REQUIRED	

>

<!ELEMENT pipe (#PCDATA)>
<!ATTLIST pipe  oraddr   CDATA #REQUIRED
	        destaddr CDATA #REQUIRED
		random   CDATA #REQUIRED
		text 	 CDATA #REQUIRED
		imsi 	 CDATA #REQUIRED
		>
				
