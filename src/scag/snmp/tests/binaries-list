scag/tests/testsnmp	testsnmp	\
   -lscag-snmp \
   -lscag-snmp-counters \
   @liblogger$(SHLIBEXT) -L$(SMSC_BUILDDIR)/bin -llogger \
   $(__LIBRARIES) \
   $(SNMP_LIBS_CORE) \
   $(SSL_LIB) $(LIBICONV) \
   -lpthread $(LIBRT) -ldl $(LIBMALLOC)

