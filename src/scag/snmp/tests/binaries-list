scag/tests/testsnmp	testsnmp	\
   @liblogger.so -L$(SMSC_BUILDDIR)/bin -llogger \
   $(__LIBRARIES) \
   $(if $(filter YES,$(SNMP)),-lscag-snmp) \
   $(SNMP_LIBS_CORE) \
  $(SSL_LIB) \
   -lpthread -lrt -ldl $(LIBMALLOC)

scag/tests/testsnmpsmsc	testsmsc	\
   @liblogger.so -L$(SMSC_BUILDDIR)/bin -llogger \
   $(__LIBRARIES) \
   $(SNMP_LIBS_CORE) \
  $(SSL_LIB) \
   -lpthread -lrt -ldl $(LIBMALLOC)
