   @liblogger$(SHLIBEXT) -L$(SMSC_BUILDDIR)/bin -llogger \
   $(__LIBRARIES) \
   $(SNMP_LIBS_CORE) \
  $(SSL_LIB) $(LIBICONV) \
   -lpthread $(LIBRT) -ldl $(LIBMALLOC)

scag/tests/testMultiSubagents testMultiSubagents \
   $(__LIBRARIES) \
   $(SNMP_LIBS_CORE) \
   $(SSL_LIB) $(LIBICONV) \
   -lpthread $(LIBRT) -ldl $(LIBMALLOC)
