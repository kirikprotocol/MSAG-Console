   -lscag-stat -lscag-util-storage -lscag-config -lscag-config-smpp \
   -lscag-config-sessn -lscag-util-properties -lscag-util-singleton \
   -L$(SMSC_BUILDDIR)/bin  -llicense-check -lutil-templates \
   -linman-interaction-msgbill -linman-interaction -linman-common \
   -lsystem-common -lsystem-smppio -lcore-threads -lutil-config -lutil \
   -lrt -ldl  -lscag-re -lscag-re-actions -lscag-re-actions-str \
