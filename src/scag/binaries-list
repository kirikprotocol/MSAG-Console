   -lscag-re-actions-datetime -lscag-re-smpp -lscag-re-session -lscag-re-http \
   -lscag-re-actions -lscag-re -lutil-xml -lscag-transport-http -lscag-stat \
   -lscag-stat -ladmin-service -lscag-stat-sacc -lscag-bill-infrastruct \
   -lscag-pers -lscag-config-pers -lscag-pers -lscag-lcm \
   \
   -lscag-lcm-base -lscag-re-base -lscag-stat-base \
   \
   -lscag-config-lcm \
   -lscag-config-base \
   -lscag-util-encodings -lutil-recoder-recode_dll -lutil-regexp -lscag-exc \
