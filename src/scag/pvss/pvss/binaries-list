# binary name                            source name     libraries         

scag/pvss                            pvss          \
        -lscag-pvss-api-pvap \
        -lscag-pvss-pvss \
        -lscag-counter-impl \
        -lscag-pvss-pvss-old \
        -lscag-pvss-api-core-server \
        -lscag-pvss-api-core \
        -lscag-pvss-api-packets \
        -lscag-pvss-base \
        -lscag-pvss-data \
	-lscag-pvss-profile \
        -lscag-pvss-common \
        -lscag-util-storage \
switch SNMP
YES: -lscag-snmp -lscag-snmp-counters \
endswitch
        -lscag-counter \
        -lscag-util-singleton \
	-lscag-util-multiplexer \
        -lscag-util-io \
	-lscag-util \
        -linformer-io \
        -llicense-check \
        -lcore-network $(SOCKETLIB) $(LIBNSL) \
        -lcore-threads \
        -lutil-mirrorfile \
        -lutil-config \
        -lutil-xml $(XERCES_LIB) \
        @liblogger$(SHLIBEXT) -L$(SMSC_BUILDDIR)/bin -llogger \
        -lutil \
        $(__LIBRARIES) \
switch SNMP
YES: $(SNMP_LIBS_CORE) \
endswitch
        $(SSL_LIB) $(LIBICONV) \
        -lpthread $(LIBRT) -ldl \
        $(LIBMALLOC)

