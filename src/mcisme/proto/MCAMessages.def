include Types.def

enum	Status
  OK 0
  SystemError 1
  SubscriberNotFound 2
end

message Response 1
  Status	status 1
  string	msg 2 opt
end

message FlushStats 2
end

message FlushStatsResp:Response 3
end

message GetStats 4
end

message GetStatsResp:Response 5
  int32		missed 1 opt
  int32		delivered 2 opt
  int32		failed 3 opt
  int32		notified 4 opt
end

message GetRunStats 6
end

message GetRunStats:Response 7
  int32		activeTasks 1 opt
  int32		inQueueSize 2 opt
  int32		outQueueSize 3 opt
  int32		inSpeed	4 opt
  int32		outSpeed 5 opt
end

message GetSched 8
  string subscriber 1
end

message GetSchedResp:Response 9
  int64		time 1 opt
  string	subscriber 2 opt
  byte		eventsCount 3 opt
  int32		lastError 4 opt
end

message TimeLineInfo 10
  int64		time 1
  int32		count 2
end

message GetSchedList 12
end

message GetSchedListResp:Response 13
  TimeLineInfo timeLines 1 array opt
end

message GetProfile 14
  string subscriber 1
end

message GetProfileResp:Response 15
  byte eventMask 1 opt
  bool inform 2 opt
  bool notify 3 opt
  bool wantNotifyMe 4 opt
  byte informTemplateId 5 opt
  byte notifyTemplateId 6 opt
end

message SetProfile 16
  string subscriber 1
  byte eventMask 2 opt
  bool inform 3 opt
  bool notify 4 opt
  bool wantNotifyMe 5 opt
  byte informTemplateId 6 opt
  byte notifyTemplateId 7 opt
end

message SetProfileResp:Response 17
end
