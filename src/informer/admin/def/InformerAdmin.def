include Types.def

enum byte ConfigOpId
  coAdd 1
  coRemove 2
  coUpdate 3
end

enum byte ConfigId
  ciSmsc 1
  ciRegion 2
  ciUser 3
end

message ConfigOp 1
  string objName 1 opt
  int32 objId 2 opt
  ConfigId cfgId 3 
  ConfigOpId op 4
end

message SetDefaultSmsc 7
  string id 1
end

message SetDefaultSmscResp 8
  int32 status 1
end

message ConfigOpResult 2
  int32 status 1
  string msg 2 opt
end

message LoggerGetCategories 3
end

message CategoryInfo
  property innerMessage=true
  string name 1
  string level 2
end

message LoggerGetCategoriesResp 4
  int32 status 1
  CategoryInfo categories 2 array opt
end

message LoggerSetCategories 5
  CategoryInfo categories 1 array
end

message LoggerSetCategoriesResp 6
  int32 status 1
end

enum byte DeliveryMode
  SMS 1
  USSD_PUSH 2
  USSD_PUSH_VLR 3
end

message SendTestSms 9
  string sourceAddr 1
  string abonent 2
  string text 3
  bool flash 4
  DeliveryMode deliveryMode 5
end

message SendTestSmsResp 10
  int32 respCode 1
end

message SmscStats
  property innerMessage=true
  string smscId 1
  int32  liveTime 2
  int32  numberOfRegions 3
  int32  maxBandwidth 4
  int32  averagingPeriod 5
  int32  currentLoad 6
  int32  pendingResponses 7
  int32  pendingReceipts 8
end

message GetSmscStats 11
end

message GetSmscStatsResp 12
  int32 status 1
  SmscStats smscStats 2 array
end
